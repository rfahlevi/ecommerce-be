{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "quantity": {
      "type": "integer",
      "min": 1,
      "required": true
    },
    "description": {
      "type": "richtext",
      "default": "description",
      "required": true
    },
    "price": {
      "type": "biginteger",
      "required": true,
      "min": "100"
    },
    "is_stock": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "distributor": {
      "type": "string",
      "required": true
    },
    "images": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true,
      "required": true
    }
  }
}
